{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "Personal blog and slides portfolio powered by Astro and Slidev",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "blog:dev": "turbo dev --filter=blog",
    "blog:build": "turbo build --filter=blog",
    "slides:dev": "turbo dev --filter=./apps/slides/*",
    "slides:build": "turbo build --filter=./apps/slides/*",
    "slides:export": "turbo export --filter=./apps/slides/*"
  },
  "keywords": ["astro", "slidev", "blog", "portfolio", "turborepo"],
  "author": "mozumasu",
  "license": "MIT",
  "dependencies": {
    "@slidev/cli": "^0.50.0-beta.6",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "playwright-chromium": "^1.48.2",
    "prettier": "^3.3.3",
    "prettier-plugin-slidev": "^1.0.5",
    "turbo": "^2.5.5",
    "typescript": "^5.4.0"
  }
}
