{
  "name": "@mozumasu/shared-components",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared slide components for presentations",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./slides/*": "./slides/*"
  },
  "files": [
    "slides",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "vue-tsc --noEmit && cp -r slides dist/",
    "dev": "vue-tsc --noEmit --watch"
  },
  "dependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vue-tsc": "^2.0.0"
  }
}